{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "../../../../Desktop/Practica_2",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": [
    "../../../../Desktop/Practica_2/node_modules"
  ]
}
